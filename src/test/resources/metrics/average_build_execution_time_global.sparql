PREFIX oslc_auto: <http://open-services.net/ns/auto#>
PREFIX ci: <http://www.smartdeveloperhub.org/vocabulary/ci#>
PREFIX dcterms: <http://purl.org/dc/terms/>

SELECT (AVG(?execution_time) as ?average_execution_time)
WHERE {
  {
    SELECT ?execution ((?finished - ?created) as ?execution_time)
    WHERE {
      ?execution dcterms:created ?created ;
                 ci:finished ?finished .
    }
  }
}
