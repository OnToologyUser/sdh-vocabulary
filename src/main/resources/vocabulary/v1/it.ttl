                                                                 rdfs:comment "issues that exist in the context of a given project"@en ;
                                                                 
                                                                 rdfs:range <http://www.smartdeveloperhub.org/vocabulary/it#Issue> ;
                                                                 
                                                                 rdfs:domain <http://www.smartdeveloperhub.org/vocabulary/it#Project> ;
                                                                 
