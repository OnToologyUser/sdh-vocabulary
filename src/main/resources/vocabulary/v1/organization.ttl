# -- Role Types ---------------------------------

org:organizationRoleType a skos:Concept;
 	 skos:prefLabel "Employee Role in Organization"@en .

org:projectRoleType a skos:Concept;
 	 skos:prefLabel "Employee Role in Project"@en .


 # -- Organization Roles

org:ctoRole a org:Role;
    rdfs:label "Chief Technology Officer"@en;    
    org:roleType org:organizationRoleType. 
   

org:headOfDevelopmentRole a org:Role;
    rdfs:label "Head of Development"@en;
    org:roleType org:organizationRoleType .


org:softwareEngineerRole a org:Role;
    rdfs:label "Developer"@en;
    org:roleType org:organizationRoleType .
    
 # -- Project Roles
org:productOwnerRole a org:Role;
    rdfs:label "Product Owner"@en;
    org:roleType org:projectRoleType .

org:scrumMasterRole a org:Role;
    rdfs:label "Scrum Master"@en;
      org:roleType org:projectRoleType.

org:devTeamRole a org:Role;
    rdfs:label "Developer Team"@en;
    org:roleType org:projectRoleType.


 #-- Organization classification
 org:CorporationType a skos:Concept;
 	skos:prefLabel "Corporation"@en.

org:DepartmentType a skos:Concept;
 	skos:prefLabel "Department"@en. 	

 #-- Organization chart
org:ISBAN a org:FormalOrganization ;
    skos:prefLabel "ISBAN" ;
    orgw3:identifier "ISBAN"^^xsd:string;
    org:classification org:CorporationType;
    org:hasUnit org:softDevelopment.

org:softDevelopment a org:OrganizationalUnit ;
    skos:prefLabel "Software Development" ;
    orgw3:identifier "SofDevEs"^^xsd:string;
    orgw3:classification org:DepartmentType;
    orgw3:purpose "develop software for ISBAN products"@en     .

